# API Configuration
VITE_API_URL=http://localhost:3000/api

# Google Maps API (for route optimization and mapping)
VITE_GOOGLE_MAPS_API_KEY=demo_key

# OpenWeatherMap API (for weather data)
VITE_OPENWEATHER_API_KEY=demo_key

# Firebase Configuration (for push notifications)
VITE_FIREBASE_API_KEY=demo_key
VITE_FIREBASE_AUTH_DOMAIN=demo.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=demo_project
VITE_FIREBASE_STORAGE_BUCKET=demo.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=demo_app_id

# Twilio Configuration (for SMS notifications)
VITE_TWILIO_ACCOUNT_SID=demo_sid
VITE_TWILIO_AUTH_TOKEN=demo_token
VITE_TWILIO_PHONE_NUMBER=+1234567890

# SendGrid Configuration (for email notifications)
VITE_SENDGRID_API_KEY=demo_key

# Application Settings
VITE_APP_NAME=RouteIQ
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_SIMULATION=true